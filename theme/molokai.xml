<?xml version="1.0" ?>
<style-scheme id="molokai" name="Molokai" version="1.0">
	<author/>
	<_description>Port of the Molokai Text Mate Theme to GTKSourceView. </_description>

  <color name="fg" value="#F2F2F8" />
  <color name="bg" value="#27281E" />
  <color name="white" value="#FFFFFF" />
  <color name="lightpurple" value="#AE81FF" />
  <color name="lightyellow" value="#E6DB74" />
  <color name="lightgreen" value="#A6E22E" />
  <color name="pink" value="#F92672" />
  <color name="lightblue" value="#79ABFF" />
  <color name="red" value="#FF0000" />
  <color name="orange" value="#FD971F" />
  <color name="grey" value="#75715E" />

  <style name="def:boolean" foreground="lightpurple" />
  <style name="def:number" foreground="lightpurple" />
  <style name="def:string" foreground="lightyellow" />
  <style name="def:constant" foreground="lightpurple" />
  <style name="cursor" foreground="white" />
  <style name="def:function" foreground="lightgreen" />
  <style name="def:keyword" foreground="pink" />
  <style name="def:statement" foreground="pink" />
  <style name="def:preprocessor" foreground="lightgreen" />
  <style name="def:specials" foreground="lightblue" />
  <style name="def:type" foreground="lightblue" />
  <style name="def:identifier" foreground="orange" />
  <style name="bracket-match" foreground="red" />
  <style name="text" foreground="fg" background="bg" />
  <style name="def:comment" foreground="grey" />

</style-scheme>
